{
	"name": "dynamicdropdown",
	"version": "0.1.0",
	"description": "A dropdown that generates a list of US Cities dynamically",
	"modern": "dist/dynamicdropdown.js",
	"source": "src/index.js",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"serve": "python -m SimpleHTTPServer",
		"clean": "rm -rf dist/",
		"bundle:dev": "microbundle watch --compress false --raw true -f modern",
		"bundle:build": "microbundle -f modern",
		"dev": "npm-run-all clean bundle:dev",
		"build": "npm-run-all clean bundle:build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/excelerondesign/DynamicDropdown.git"
	},
	"author": {
		"name": "frankie",
		"url": "https://github.com/frankie-tech"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/excelerondesign/DynamicDropdown/issues"
	},
	"homepage": "https://github.com/excelerondesign/DynamicDropdown#readme",
	"devDependencies": {
		"husky": "^4.3.0",
		"microbundle": "^0.12.4",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.1.2",
		"pretty-quick": "^3.0.2"
	},
	"husky": {
		"hooks": {
			"precommit": "pretty-quick --staged"
		}
	},
	"mangle": {
		"regex": "^_"
	},
	"browserslist": [
		"Edge 15",
		"supports es6-generators"
	]
}
